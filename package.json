    "cookie-session": "^2.0.0-beta.3",
    "debug": "^4.1.0",
    "express": "^4.16.3",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-github2": "^0.1.11",
    "passport-google-oauth2": "^0.1.6",
    "passport-twitter": "^1.0.4",
    "rethinkdbdash": "^2.3.31"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.4",
    "rethinkdb-migrate": "^1.4.0",
    "standard": "^12.0.1"
  }
